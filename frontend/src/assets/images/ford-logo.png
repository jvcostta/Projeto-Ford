<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 200 80" width="200" height="80">
  <!-- Ford logo recreation in SVG -->
  <defs>
    <linearGradient id="fordBlue" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#003478;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1766a6;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background oval -->
  <ellipse cx="100" cy="40" rx="95" ry="35" fill="url(#fordBlue)" stroke="#fff" stroke-width="2"/>
  
  <!-- Ford text -->
  <text x="100" y="52" font-family="Arial, sans-serif" font-size="32" font-weight="bold" fill="white" text-anchor="middle">FORD</text>
  
  <!-- Decorative elements -->
  <circle cx="25" cy="20" r="3" fill="white" opacity="0.6"/>
  <circle cx="175" cy="20" r="3" fill="white" opacity="0.6"/>
  <circle cx="25" cy="60" r="3" fill="white" opacity="0.6"/>
  <circle cx="175" cy="60" r="3" fill="white" opacity="0.6"/>
</svg>
